{
  "name": "notas-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "serve -s build",
    "heroku-postbuild": "npm run build",
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "prettier": "prettier --write \"**/*.{ts,js,json}\""
  },
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "formik": "^1.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serve": "^11.3.0",
    "yup": "^0.28.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/react-loadable": "^5.5.3",
    "@types/react-router-dom": "^5.1.3",
    "@vtex/prettier-config": "^0.1.3",
    "@vtex/tsconfig": "^0.4.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-vtex": "^12.3.0",
    "eslint-config-vtex-react": "^6.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vtex": "^1.0.3",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
