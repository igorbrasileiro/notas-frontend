    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vtex": "^1.0.3",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
