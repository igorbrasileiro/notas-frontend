    "formik": "^1.3.1",
    "graphql": "^15.0.0",
    "graphql.macro": "^1.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serve": "^11.3.0",
    "uuid": "^7.0.2",
    "yup": "^0.28.3"
  },
  "husky": {
    "hooks": {
