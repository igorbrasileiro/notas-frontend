    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/uuid": "^7.0.2",
    "@types/yup": "^0.26.33",
    "@vtex/prettier-config": "^0.1.3",
    "@vtex/tsconfig": "^0.4.3",
